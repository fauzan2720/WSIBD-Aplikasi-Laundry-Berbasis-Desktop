	<queryString>
		<![CDATA[SELECT
     tb_transaksi.`id_transaksi` AS tb_transaksi_id_transaksi,
     tb_transaksi.`tgl_terima` AS tb_transaksi_tgl_terima,
     tb_transaksi.`tgl_selesai` AS tb_transaksi_tgl_selesai,
     tb_transaksi.`id_member` AS tb_transaksi_id_member,
     tb_transaksi.`id_laundry` AS tb_transaksi_id_laundry,
     tb_member.`id_member` AS tb_member_id_member,
     tb_member.`nama` AS tb_member_nama,
     tb_member.`jenis_kelamin` AS tb_member_jenis_kelamin,
     tb_member.`tgl_daftar` AS tb_member_tgl_daftar,
     tb_member.`no_telp` AS tb_member_no_telp,
     tb_member.`alamat` AS tb_member_alamat,
     tb_laundry.`id_laundry` AS tb_laundry_id_laundry,
     tb_laundry.`id_jns_laundry` AS tb_laundry_id_jns_laundry,
     tb_laundry.`jml_potong` AS tb_laundry_jml_potong,
     tb_laundry.`jml_kg` AS tb_laundry_jml_kg,
     tb_laundry.`id_jnsplain` AS tb_laundry_id_jnsplain,
     tb_laundry.`total_harga` AS tb_laundry_total_harga,
     tb_jns_laundry.`id_jns_laundry` AS tb_jns_laundry_id_jns_laundry,
     tb_jns_laundry.`nama_jns_laundry` AS tb_jns_laundry_nama_jns_laundry,
     tb_jns_laundry.`harga` AS tb_jns_laundry_harga,
     tb_jns_plain.`id_jns_plain` AS tb_jns_plain_id_jns_plain,
     tb_jns_plain.`nama_plain` AS tb_jns_plain_nama_plain,
     tb_jns_plain.`harga` AS tb_jns_plain_harga
FROM
     `tb_member` tb_member INNER JOIN `tb_transaksi` tb_transaksi ON tb_member.`id_member` = tb_transaksi.`id_member`
     INNER JOIN `tb_laundry` tb_laundry ON tb_transaksi.`id_laundry` = tb_laundry.`id_laundry`
     INNER JOIN `tb_jns_laundry` tb_jns_laundry ON tb_laundry.`id_jns_laundry` = tb_jns_laundry.`id_jns_laundry`
     INNER JOIN `tb_jns_plain` tb_jns_plain ON tb_laundry.`id_jnsplain` = tb_jns_plain.`id_jns_plain`]]>
	</queryString>
